mixin header-rent-auto(options={ mainPage:"", catalogue:"", aboutUs:""})
    - let {mainPage, catalogue, aboutUs} = options;
    .header-rent-auto
        .header-rent-auto__logotip-container
            .header-rent-auto__logotip logotip
        .header-rent-auto__menu-container
            +button({
              size: `navigation`,
              color: `green`,
              name: `js-ripple`,
              title: `${mainPage}`
            })
            +button({
              size: `navigation`,
              color: `green`,
              name: `js-ripple`,
              title: `${catalogue}`
            })
            +button({
              size: `navigation`,
              color: `green`,
              name: `js-ripple`,
              title: `${aboutUs}`
            })