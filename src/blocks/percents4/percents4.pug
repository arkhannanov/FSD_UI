mixin percents4(options={number:""})
    -let {number} = options; 
    .percents4
        svg(width="100%",height="100%", viewBox="0 0 100 100")
            circle(class="segment", cx="50", cy="50", r="45", fill="transparent", stroke="#e75735", stroke-width="5", stroke-dasharray="251.514 31.086", stroke-dashoffset="70.65")
            text(x="50", y="50", class="percents4__number")= number
