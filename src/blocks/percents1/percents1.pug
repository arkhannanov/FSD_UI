mixin percents1(options={number:""})
    -let {number} = options; 
    .percents1
        svg(width="100%",height="100%", viewBox="0 0 100 100")
            circle(class="segment", cx="50", cy="50", r="45", fill="transparent")
            text(x="50", y="50", class="percents1__number")= number
